    <parameterMap id="SkuLocationParameters" class="Hashtable">
      <parameter property="WarehouseId" column="WarehouseId" />
      <parameter property="SkuId" column="SkuId" />
      <parameter property="AreaType" column="AreaType" />
    </parameterMap>
